<window id="115">
  <defaultcontrol always="true">901</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <visible>!Window.IsVisible(fullscreeninfo) + [VideoPlayer.IsFullscreen | Window.IsVisible(visualisation)] + [Player.Seeking | Player.DisplayAfterSeek | Player.paused | Player.Forwarding | Player.rewinding]</visible>

  <animation type="WindowOpen">
    <effect type="fade" start="0" end="100" time="300" />
  </animation>
  <animation type="WindowClose">
    <effect type="fade" start="100" end="0" time="300" />
  </animation>

  <controls>

    <control type="image">
      <description>black bar</description>
      <posx>0</posx>
      <posy>440</posy>
      <width>1280</width>
      <height>232</height>
      <fadetime>200</fadetime>
      <texture background="true">tile.png</texture>
      <colordiffuse>White80</colordiffuse>
      <animation type="VisibleChange">
        <effect type="fade" start="0" end="100" time="600" />
      </animation>
      <visible>Player.HasVideo + [VideoPlayer.Content(Movies) | !VideoPlayer.Content(Movies)]</visible>
    </control>

    <control type="group">
      <visible>Player.HasVideo</visible>
      <control type="progress">
        <description>Progressbar</description>
        <posx>0</posx>
        <posy>715</posy>
        <width>1280</width>
        <height>5</height>
        <info>Player.Progress</info>
        <texturebg>progressmidgreen.png</texturebg>
        <midtexture>progresslime.png</midtexture>
      </control>

      <control type="label">
        <description>Player Times</description>
        <posx>30</posx>
        <posy>670</posy>
        <width>120</width>
        <label>[COLOR Accent]$INFO[Player.Time][/COLOR]</label>
        <align>left</align>
        <font>Medium</font>
      </control>

      <control type="label">
        <description>Player Times</description>
        <posx>1250</posx>
        <posy>670</posy>
        <width>120</width>
        <label>$INFO[Player.TimeRemaining,-]</label>
        <align>right</align>
        <font>Medium</font>
        <textcolor>white</textcolor>
      </control>
    </control>

    <control type="group">
      <visible>Player.HasAudio</visible>
      <posx>140</posx>
      <posy>570</posy>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]paused[/B]</label>
        <visible>player.paused</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]FFx2[/B]</label>
        <visible>player.forwarding2x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]FFx4[/B]</label>
        <visible>player.forwarding4x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]FFx8[/B]</label>
        <visible>player.forwarding8x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]FFx16[/B]</label>
        <visible>player.forwarding16x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>FFx32</label>
        <visible>player.forwarding32x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]RWx2[/B]</label>
        <visible>player.rewinding2x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]RWx4[/B]</label>
        <visible>player.rewinding4x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]RWx8[/B]</label>
        <visible>player.rewinding8x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]RWx16[/B]</label>
        <visible>player.rewinding16x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>100</posy>
        <width>1000</width>
        <align>center</align>
        <label>[B]RWx32[/B]</label>
        <visible>player.rewinding32x</visible>
      </control>
    </control>

    <control type="group">
      <visible>Player.HasVideo</visible>
      <posx>0</posx>
      <posy>0</posy>

      <control type="image">
        <description>Non Movie cover image</description>
        <posx>148</posx>
        <posy>440</posy>
        <width>156</width>
        <height>232</height>
        <fadetime>200</fadetime>
        <texture background="true">$INFO[Player.Art(tvshow.poster)]</texture>
        <bordertexture border="5">white_box.png</bordertexture>
        <bordersize>5</bordersize>
        <visible>!VideoPlayer.Content(Movies)</visible>
      </control>

      <control type="image">
        <description>Movie cover image</description>
        <posx>148</posx>
        <posy>440</posy>
        <width>156</width>
        <height>232</height>
        <fadetime>400</fadetime>
        <texture background="true">$INFO[VideoPlayer.Cover]</texture>
        <bordertexture border="5">white_box.png</bordertexture>
        <bordersize>5</bordersize>
        <visible>VideoPlayer.Content(Movies)</visible>
      </control>

    </control>

    <control type="group">
      <visible>Player.HasVideo</visible>
      <posx>325</posx>
      <posy>515</posy>

      <control type="label">
        <description>Now Playing</description>
        <posx>0</posx>
        <posy>-5</posy>
        <width>955</width>
        <label>$INFO[VideoPlayer.ChannelName,, ][COLOR Accent]$INFO[VideoPlayer.Title][/COLOR] [LOWERCASE]$INFO[VideoPlayer.Year][/LOWERCASE]</label>
        <scroll>true</scroll>
        <font>Home_menu</font>
        <align>left</align>
        <visible>[VideoPlayer.Content(Movies) | !VideoPlayer.Content(Movies)] + !Window.IsVisible(fullscreeninfo) + !Window.IsVisible(videoosd)</visible>
      </control>

      <control type="label">
        <description>Now Playing</description>
        <posx>0</posx>
        <posy>32</posy>
        <width>700</width>
        <label>[LOWERCASE]$INFO[VideoPlayer.Tagline][/LOWERCASE][COLOR Accent]$INFO[VideoPlayer.EndTime,[COLOR white]End Time: [/COLOR]][/COLOR] [COLOR Accent]$INFO[VideoPlayer.NextTitle,[COLOR white]On Next: [/COLOR]][/COLOR]</label>
        <scroll>true</scroll>
        <font>Medium</font>
        <align>left</align>
      </control>

      <control type="label">
        <description>Now Playing</description>
        <posx>0</posx>
        <posy>32</posy>
        <width>700</width>
        <label>[COLOR Accent]$INFO[VideoPlayer.Director,[COLOR White]director: [/COLOR]][/COLOR]</label>
        <scroll>true</scroll>
        <font>Medium</font>
        <align>left</align>
        <visible>VideoPlayer.Content(Movies) + IsEmpty(VideoPlayer.Tagline) + !IsEmpty(VideoPlayer.Director)</visible>
      </control>

      <control type="label">
        <description>Now Playing</description>
        <visible>IsEmpty(VideoPlayer.Tagline)</visible>
        <posx>0</posx>
        <posy>32</posy>
        <width>700</width>
        <label>$INFO[VideoPlayer.TVShowTitle][COLOR Accent] $INFO[VideoPlayer.Season,Season ]$INFO[VideoPlayer.Episode, Episode ][/COLOR]</label>
        <scroll>true</scroll>
        <font>Medium</font>
        <align>left</align>
        <visible>VideoPlayer.Content(Episodes)</visible>
      </control>

      <control type="label">
        <description>Now Playing</description>
        <visible>IsEmpty(VideoPlayer.Tagline)</visible>
        <posx>60</posx>
        <posy>32</posy>
        <width>700</width>
        <label>$INFO[VideoPlayer.Artist] [COLOR Accent]$INFO[VideoPlayer.Album][/COLOR]</label>
        <scroll>true</scroll>
        <font>Medium</font>
        <align>left</align>
        <visible>VideoPlayer.Content(MusicVideos)</visible>
      </control>

      <control type="image">
        <description>Rating image</description>
        <posx>0</posx>
        <posy>70</posy>
        <width>130</width>
        <height>25</height>
        <aspectratio align="left">keep</aspectratio>
        <texture>other_textures/ColorRating/$INFO[Player.StarRating]</texture>
      </control>

      <control type="label">
        <description>Now Playing</description>
        <visible>IsEmpty(VideoPlayer.Tagline)</visible>
        <posx>0</posx>
        <posy>62</posy>
        <width>830</width>
        <label>Currently: [COLOR Accent]$INFO[System.Time(hh:mm)] $INFO[System.Time(xx)][/COLOR]</label>
        <scroll>true</scroll>
        <font>Medium</font>
        <align>left</align>
        <visible>!IsEmpty(VideoPlayer.EndTime)</visible>
      </control>

    </control>

    <control type="group">
      <visible>Player.HasVideo</visible>
      <posx>325</posx>
      <posy>570</posy>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]paused[/B][/UPPERCASE]</label>
        <visible>player.paused</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]FFx2[/B][/UPPERCASE]</label>
        <visible>player.forwarding2x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]FFx4[/B][/UPPERCASE]</label>
        <visible>player.forwarding4x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]FFx8[/B][/UPPERCASE]</label>
        <visible>player.forwarding8x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]FFx16[/B][/UPPERCASE]</label>
        <visible>player.forwarding16x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>FFx32</label>
        <visible>player.forwarding32x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]RWx2[/B][/UPPERCASE]</label>
        <visible>player.rewinding2x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]RWx4[/B][/UPPERCASE]</label>
        <visible>player.rewinding4x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]RWx8[/B][/UPPERCASE]</label>
        <visible>player.rewinding8x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]RWx16[/B][/UPPERCASE]</label>
        <visible>player.rewinding16x</visible>
      </control>

      <control type="label">
        <posx>0</posx>
        <posy>40</posy>
        <width>1000</width>
        <align>left</align>
        <label>[UPPERCASE][B]RWx32[/B][/UPPERCASE]</label>
        <visible>player.rewinding32x</visible>
      </control>

    </control>

    <control type="group">
      <posx>515</posx>
      <posy>324</posy>

      <visible>!IsEmpty(Player.CacheLevel) + [!StringCompare(Player.CacheLevel,100) + !StringCompare(Player.CacheLevel,0)] + !Player.paused</visible>

      <control type="image">
        <description>background image</description>
        <posx>0</posx>
        <posy>0</posy>
        <width>250</width>
        <height>78</height>
        <texture border="20">button.png</texture>
      </control>

      <control type="image">
        <description>Busy animation</description>
        <posx>20</posx>
        <posy>20</posy>
        <width>32</width>
        <height>32</height>
        <texture>other_textures/busy.png</texture>
        <aspectratio>keep</aspectratio>
        <animation effect="rotate" start="360" end="0" center="36,36" time="1200" loop="true" condition="true">conditional</animation>
      </control>

      <control type="label" id="24">
        <description>buffering label</description>
        <posx>70</posx>
        <posy>20</posy>
        <width>200</width>
        <height>32</height>
        <label>$LOCALIZE[15107] $INFO[Player.CacheLevel]%</label>
        <align>left</align>
        <font>Medium</font>
      </control>

    </control>
  </controls>
</window>
